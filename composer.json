{
  "name": "kojit2009/laravel-roles-permissions",
  "description": "Simple but powerful package for handling roles and permissions in Laravel",
  "keywords": [
    "laravel",
    "roles",
    "permissions",
    "auth",
    "acl",
    "rbac",
    "centeron",
    "security"
  ],
  "homepage": "https://github.com/centeron/laravel-roles-permissions",
  "license": "MIT",
  "authors": [
    {
      "name": "Shekhovtsov Yuri",
      "email": "shekhovtsovyuri@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "laravel/framework": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit" : "^8.5"
  },
  "autoload": {
    "psr-4": {
      "Centeron\\Permissions\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Centeron\\Permissions\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Centeron\\Permissions\\ServiceProvider"
      ]
    }
  }
}


